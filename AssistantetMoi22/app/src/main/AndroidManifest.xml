<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.bak.assistantetmoi2">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logo_am"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_am"
        android:supportsRtl="true"
        android:theme="@style/Theme.AssistantEtMoi"
        tools:targetApi="32">

        <activity
            android:name=".HistorytActivity"
            android:exported="true"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".HelpUserActivity"
            android:exported="true"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".DisponibilityHelperActivity"
            android:exported="true"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".OnlineHelpActivity"
            android:exported="true"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".RegisterTwoHelperActivity"
            android:exported="true"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".RegisterTwoUserActivity"
            android:exported="true"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".RegisterHelperActivity"
            android:exported="true"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".RegisterUserActivity"
            android:exported="true"
            android:screenOrientation="portrait"/>

        <activity
            android:name="com.bak.assistantetmoi2.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

    </application>

</manifest>